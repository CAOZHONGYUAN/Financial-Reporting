<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fms.dao.PropertyDao">
    <insert id="insert" parameterType="com.fms.entity.Property">
    insert into Property (useFor, cost, depict, billId, payeeUserId,invoice)
    values (#{usefor}, #{cost}, #{depict}, #{billid}, #{payeeuserid},#{invoice})
    </insert>

    <!--修改账单-->
    <update id="update" parameterType="com.fms.entity.Property">
        update property
        set useFor=#{usefor},depict=#{depict},payeeUserId=#{payeeuserid},cost=#{cost}
        WHERE billId=#{billid}
    </update>
    <select id="selectByBillid" resultType="Map">
        select * from property where billId = #{billId}
    </select>
</mapper>